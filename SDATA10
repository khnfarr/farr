#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* left;
    Node* right;
};

Node* newNode(int data) {
    Node* node = new Node();
    node->data = data;
    node->left = NULL;
    node->right = NULL;
    return node;
}

Node* insert(Node* root, int data) {
    if (root == NULL)
        return newNode(data);

    if (data < root->data)
        root->left = insert(root->left, data);
    else if (data > root->data)
        root->right = insert(root->right, data);

    return root;
}

void inorder(Node* root) {
    if (root != NULL) {
        inorder(root->left);
        cout << root->data << " ";
        inorder(root->right);
    }
}

int main() {
    Node* root = NULL;

    int data[] = {50, 19, 18, 57, 95, 77, 39, 61, 23, 56};
    int n = 10;

    for (int i = 0; i < n; i++) {
        root = insert(root, data[i]);
    }

    cout << "Hasil Traversal Inorder BST: ";
    inorder(root);

    return 0;
}
